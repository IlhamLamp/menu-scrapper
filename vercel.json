{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/menu/?(.*)",
            "dest": "src/menu/menu.controller.ts",
            "methods": [ "GET", "POST", "PUT", "DELETE" ]
        },
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [ "GET", "POST", "PUT", "DELETE" ]
        }
    ]
}